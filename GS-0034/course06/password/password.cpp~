#include <iostream>
#include <cstdio>
#include <string>

using namespace std;

int main()
{
  freopen("password.in", "r", stdin);
  freopen("password.out", "w", stdout);

  string password;
  string step1;
  getline(cin, password);

  for (int i = 0; i < password.size(); i++)
  {
    char ch = password[i];
    if (ch >= 'A' && ch <= 'C' || ch >= 'a' && ch <= 'c')
    {
      step1[i] = password[i] + 23;
    }
    else
    {
      step1[i] = password[i] - 3;
    }
  }

  

  
  return 0;
}
