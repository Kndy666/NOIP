#include <iostream>
#include <cstdio>

using namespace std;

int main()
{
  freopen("in.in", "r", stdin);
  freopen("out.out", "w", stdout);

  string a;
  cin >> a;
  
  string b;
  for (int i = a.size(); i > 0; i--)
  {
    b += a[i];
  }

  if (a == b)cout << "YES" << endl;
  else cout << "NO" << endl;


}
