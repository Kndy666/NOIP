#include <iostream>

using namespace std;

int main()
{
  double a, ans;

  cin >> a;

  if (0 <= a < 3500)ans = 0;
  else if (3500 <= a < 5000)ans = (a - 3500) * 0.03;
  else if (5000 <= a < 8000)ans = 1500 * 0.03 + (a - 5000) * 0.1;
  else if (8000 <= a < 12500)ans = 1500 * 0.03 + 3000 * 0.1 + (a - 8000) * 0.2;
